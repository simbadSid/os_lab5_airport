CC     	= gcc
EXEC   	= planeManager
CFLAGS 	= -Wall -Werror -g -pthread
all   	: $(EXEC)










#-----------------------------------------------------------------------------------------------------------
# --------------------------------------- Executables a creer -----------------------------------------------
#-----------------------------------------------------------------------------------------------------------
planeManager		: planeManager.o ../sharedFile/shared.o
					$(CC) $(CFLAGS) -o $@ $^

planeManager.o		: planeManager.c
					$(CC) $(CFLAGS) -c -o $@ $^

../sharedFile/shared.o	: ../sharedFile/shared.c
					$(CC) $(CFLAGS) -c -o $@ $^
#-----------------------------------------------------------------------------------------------------------
#---------------------------------------- Methodes generales -----------------------------------------------
#-----------------------------------------------------------------------------------------------------------
.PHONY	: clean mrproper
clean:
	rm -f *.o
	rm -f ../sharedFile/*.o
mrproper: clean
	rm -rf $(EXEC)
